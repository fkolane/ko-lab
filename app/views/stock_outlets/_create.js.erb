$("ul.errors").html("");
<% if @stock_outlet.errors.any? %>
  <% @stock_outlet.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".stock-outlets").html("<%= escape_javascript(render 'index') %>");
  $("#stock-outlet-modal").modal("hide");
 
<% end %>