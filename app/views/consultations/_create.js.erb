$("ul.errors").html("");
<% if @consultation.errors.any? %>
  <% @consultation.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".consultations").html("<%= escape_javascript(render 'index') %>");
  $("#consultation-modal").modal("hide");
 
<% end %>