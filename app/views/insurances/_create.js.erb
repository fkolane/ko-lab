$("ul.errors").html("");
<% if @insurance.errors.any? %>
  <% @insurance.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".insurances").html("<%= escape_javascript(render 'index') %>");
  $("#insurance-modal").modal("hide");
 
<% end %>