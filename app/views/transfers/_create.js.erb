$("ul.errors").html("");
<% if @transfer.errors.any? %>
  <% @transfer.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".transfers").html("<%= escape_javascript(render 'index') %>");
  $("#transfer-modal").modal("hide");
 
<% end %>