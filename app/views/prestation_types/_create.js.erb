$("ul.errors").html("");
<% if @prestation_type.errors.any? %>
  <% @prestation_type.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".prestation-types").html("<%= escape_javascript(render 'index') %>");
  $("#prestation-type-modal").modal("hide");
 
<% end %>