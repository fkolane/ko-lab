<p id="notice"><%= notice %></p>

<h1>Analyse Elements</h1>

<table>
  <thead>
    <tr>
      <th>Analyse</th>
      <th>Name</th>
      <th>Normal value</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analyse_elements.each do |analyse_element| %>
      <tr>
        <td><%= analyse_element.analyse %></td>
        <td><%= analyse_element.name %></td>
        <td><%= analyse_element.normal_value %></td>
        <td><%= analyse_element.status %></td>
        <td><%= link_to 'Show', analyse_element %></td>
        <td><%= link_to 'Edit', edit_analyse_element_path(analyse_element) %></td>
        <td><%= link_to 'Destroy', analyse_element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analyse Element', new_analyse_element_path %>
